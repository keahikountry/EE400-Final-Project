{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Arduino Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/nano_ble33_sense_camera.ino.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "arm-none-eabi-gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Arduino Compile Only - Camera"
        }
    ]
}
